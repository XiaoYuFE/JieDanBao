page{
  background:#f6f6f6; 
}
.caption-top {
  height: 64rpx;
  line-height: 64rpx;
  background: #d0f4e5;
  text-align: center;
  color: #14c67d;
  font-size: 28rpx;
}

.customer-info {
  padding: 50rpx 30rpx;
  position: relative;
  background:#fff; 
  margin-bottom:20rpx
}

.customer-info .tit {
  color:#1a1a1a;
  font-size:28rpx;
}

.customer-info .phone {
  color: #138ce1;
  font-size: 40rpx;
  text-align: center;
}

.customer-info .tip-1 {
  width: 230rpx;
  height: 131rpx;
  position: absolute;
  right: 30px;
  top: 15rpx;
  background: url("http://m3.xiaoyu.com/img/fish_txt.png") no-repeat top center;
  background-size: 100% 100%;
}

.customer-info .tip-sx {
  width: 230rpx;
  height: 131rpx;
  position: absolute;
  right: 30px;
  top: 15rpx;
  background: url("http://m3.xiaoyu.com/img/sx_txt.png") no-repeat top center;
  background-size: 100% 100%;
}

.customer-info.step-end .phone {
  color: #ccc;
}

.customer-info.step-end .txt-1 {
  display: block;
}

.order-info {
  background: #f6f6f6;
  padding: 20rpx 30rpx;
}

.order-info .txt-1 {
  color: #666;
  font-size: 24rpx;
  line-height: 2;
}

.order-steps {
  position: relative;
  
}




.order-steps.step-1 .step-list .item-cell-1 .txt, 


.order-steps.step-2 .step-list .item-cell-1 .txt, 
.order-steps.step-2 .step-list .item-cell-2 .txt
{
  color: #138ce1;
}

.order-steps.step-1 .step-list .item-cell-1::after{
 
    background:#138ce1; 
}
.order-steps.step-2 .step-list .item-cell-1::after{
 
    background:#138ce1; 
}
.order-steps.step-2 .step-list .item-cell-2::after{
 
    background:#138ce1; 
}



.order-steps.step-1 .item-cell-1 .circle .circle-1 {
  background-color: #138ce1;
}

.order-steps.step-1 .item-cell-1 .circle .circle-2 {
  background-color: #84c3ef;
}

.order-steps.step-1 .item-cell-1 .circle .circle-3 {
  background-color: #d0e8f9;
}

.order-steps.step-2 .item-cell-1 .circle .circle-1 {
  background-color: #138ce1;
}

.order-steps.step-2 .item-cell-1 .circle .circle-2 {
  background-color: #84c3ef;
}

.order-steps.step-2 .item-cell-1 .circle .circle-3 {
  background-color: #d0e8f9;
}

.order-steps.step-2 .item-cell-2 .circle .circle-1 {
  background-color: #138ce1;
}

.order-steps.step-2 .item-cell-2 .circle .circle-2 {
  background-color: #84c3ef;
}

.order-steps.step-2 .item-cell-2 .circle .circle-3 {
  background-color: #d0e8f9;
}

.order-steps .step-list {
  margin-top: 15rpx;
}

.order-steps .step-list .item-cell {
  float: left;
  width:50%;
  position:relative; 
}
.order-steps .step-list .item-cell::after{
  content:"";
  height:1rpx;
  background:#999999;
  position:absolute;
  top:10rpx;
  width:100%;     
}
.order-steps .step-list .item-cell-1::after{
  left:50rpx;
}

.order-steps .step-list .item-cell-2::after{
  right:50rpx;
}


.order-steps .step-list .item-cell .txt {
  color: #3c4050;
  font-size: 24rpx;
  text-align: left;
  display: block;
  margin-top: 35rpx;
  clear:bottom; 
}
.order-steps .step-list .item-cell-1 .txt{
  padding-left: 15rpx;
}

.order-steps .step-list .item-cell-2 .txt{
  text-align: right;
  padding-right: 30rpx;
}

.order-steps .step-list .item-cell-1 .circle {
   margin-left: 40rpx;
   float:left; 
   position:relative;
   z-index: 10; 
}

.order-steps .step-list .item-cell-2 {
  text-align: right;
}
.order-steps .step-list .item-cell-2 .circle {
   margin-right: 40rpx;
   float:right; 
   

}

.order-steps .step-list .item-cell .circle {
  position: relative;
  width: 24rpx;
  height: 24rpx;
  z-index: 10;
 
}

.order-steps .step-list .item-cell .circle-1 {
  width: 12rpx;
  height: 12rpx;
  position: absolute;
  left: 50%;
  top: 50%;
  transform: translate(-50%, -50%);
  overflow: hidden;
  background: #999;
  border-radius: 100%;
}

.order-steps .step-list .item-cell .circle-2 {
  width: 18rpx;
  height: 18rpx;
  position: absolute;
  left: 50%;
  top: 50%;
  transform: translate(-50%, -50%);
  overflow: hidden;
  background: #c5c5c5;
  border-radius: 100%;
}

.order-steps .step-list .item-cell .circle-3 {
  width: 24rpx;
  height: 24rpx;
  position: absolute;
  left: 50%;
  top: 50%;
  transform: translate(-50%, -50%);
  overflow: hidden;
  background: #e3e3e3;
  border-radius: 100%;
}


.fixed-bottom {
  position: fixed;
  bottom: 0; 
  width: 100%;
  box-sizing: border-box;
  background: #fff;
  padding:10rpx 30rpx; 
}
.fixed-bottom.device-ipx{
  bottom:25rpx;
}

.fixed-bottom .phone {
  float: left;
  line-height: 80rpx;
  color: #3c4050;
  font-size: 28rpx;
}

.fixed-bottom .phone .icon-txt {
  font-size: 52rpx;
  color: #138ce1;
  vertical-align: middle;
}

.fixed-bottom .phone {
  width:40%; 
  text-align: center;
}

.fixed-bottom .hand-txt {
  float: right;
  background: #138ce1;
  height: 80rpx;
  line-height:80rpx; 
  color: #fff;
  width:60%; 
  text-align:center; 
  border-radius:5rpx; 
}
.fixed-bottom .hand-txt.disabled{
  background:#cccccc; 
}

.fixed-bottom .hand-txt .txt-1 {
  font-size: 28rpx;
  text-align: center;
  display: block;
  
}
.fixed-bottom .hand-txt .txt-2 {
    font-size: 20rpx;
    line-height:1; 
 
}

.fixed-bottom-seat{
  height:80rpx; 
}

.fixed-bottom-seat.device-ipx{
  height:105rpx; 
}

.gobackhome{
  position:fixed;
  right:30rpx;
  bottom:20%;
  width:70rpx;
  height:70rpx;
  line-height:70rpx; 
  background:rgba(0,0,0,0.5);
  border-radius:100%;
  text-align:center;      
}

.gobackhome navigator{
  color:#fff;
  font-size:34rpx;  
}








