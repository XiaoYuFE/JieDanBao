.dialog-new-result {
  background: #fff;
  border-radius: 10rpx;
  position: fixed;
  left: 50%;
  margin-left: -315rpx;
  top: 30%;
  width: 630rpx;
  box-sizing: border-box;
  z-index: 100;
  padding: 40rpx 40rpx 60rpx;
}

.dialog-new-result-mask {
  position: fixed;
  left: 0;
  right: 0;
  top: 0;
  bottom: 0;
  z-index: 99;
  background-color: rgba(0, 0, 0, 0.8);
}

.dialog-new-result .item-btn {
  width: 380rpx;
  height: 88rpx;
  overflow: hidden;
  line-height: 88rpx;
  text-align: center;
  background: #3ac6b8;
  border-radius: 50rpx;
  color: #fff;
  margin: 40rpx auto 0;
}

.order-steps .step-list {
  margin-top: 15rpx;
}

.order-steps .step-list .item-cell {
  float: left;
  width: 14.28%;
}

.order-steps .step-list .item-cell .txt {
  color: #3c4050;
  font-size: 24rpx;
  text-align: center;
  display: block;
  margin-top: 15rpx;
}

.order-steps .step-list .item-cell .circle {
  position: relative;
  width: 24rpx;
  height: 24rpx;
  margin: 0 auto;
}

.order-steps .step-list .item-cell .circle-1 {
  width: 12rpx;
  height: 12rpx;
  position: absolute;
  left: 50%;
  top: 50%;
  transform: translate(-50%, -50%);
  overflow: hidden;
  background: #999;
  border-radius: 100%;
}

.order-steps .step-list .item-cell .circle-2 {
  width: 18rpx;
  height: 18rpx;
  position: absolute;
  left: 50%;
  top: 50%;
  transform: translate(-50%, -50%);
  overflow: hidden;
  background: #c5c5c5;
  border-radius: 100%;
}

.order-steps .step-list .item-cell .circle-3 {
  width: 24rpx;
  height: 24rpx;
  position: absolute;
  left: 50%;
  top: 50%;
  transform: translate(-50%, -50%);
  overflow: hidden;
  background: #e3e3e3;
  border-radius: 100%;
}

