.fixed-bottom {
  position: fixed;
  bottom: 0;
  width: 100%;
  box-sizing: border-box;
  background: #fff;
  padding: 10rpx 30rpx;
}

.fixed-bottom.device-ipx {
  bottom: 25rpx;
}

.fixed-bottom .phone {
  float: left;
  line-height: 80rpx;
  color: #3c4050;
  font-size: 28rpx;
}

.fixed-bottom .phone .icon-txt {
  font-size: 52rpx;
  color: #14c67d;
  vertical-align: middle;
}

.fixed-bottom .phone {
  width: 40%;
  text-align: center;
}

.fixed-bottom .hand-txt {
  float: right;
  background: #14c67d;
  height: 80rpx;
  line-height: 80rpx;
  color: #fff;
  width: 60%;
  text-align: center;
}

.fixed-bottom .hand-txt.disabled {
  background: #ccc;
}

.fixed-bottom .hand-txt .txt-1 {
  font-size: 28rpx;
  text-align: center;
  display: block;
}

.fixed-bottom .hand-txt .txt-2 {
  font-size: 20rpx;
  line-height: 1;
}

.fixed-bottom-seat {
  height: 80rpx;
}

.fixed-bottom-seat.device-ipx {
  height: 105rpx;
}

