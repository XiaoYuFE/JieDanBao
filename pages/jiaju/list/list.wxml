

<view class='header-top'>
  <view class="plist-nav-top">
    <view bindtap='navMainHandle' data-ordertype="wjd"  data-active="{{orderType=='wjd' ? 'active' :''}}" class="item-cell {{orderType=='wjd' ? 'active' :''}}">新订单</view>
    <view bindtap='navMainHandle' data-ordertype="clz" data-active="{{orderType=='clz' ? 'active' :''}}" class="item-cell {{orderType=='clz' ? 'active' :''}}">处理中</view>
    <view bindtap='navMainHandle' data-ordertype="yjs" data-active="{{orderType=='yjs' ? 'active' :''}}" class="item-cell {{orderType=='yjs' ? 'active' :''}}">已结束</view>
  </view>
  <view class='plist-nav-sub' wx:if="{{orderType=='clz' ? true :false}}">
    <view class='item-cell {{orderStep=="all" ? "active" :""}}'>
      <text bindtap='navSubHandle' data-orderstep="all"  data-active="{{orderStep=='all' ? 'active' :''}}">全部</text>
    </view>
    <view class='item-cell {{orderStep=="dlf" ? "active" :""}}'>
      <text bindtap='navSubHandle' data-orderstep="dlf"  data-active="{{orderStep=='dlf' ? 'active' :''}}">待量房</text>
    </view>
    <view class='item-cell {{orderStep=="dsj" ? "active" :""}}'>
      <text bindtap='navSubHandle'  data-orderstep="dsj"  data-active="{{orderStep=='dsj' ? 'active' :''}}">待设计</text>
    </view>
    <view class='item-cell {{orderStep=="dqy" ? "active" :""}}'>
      <text bindtap='navSubHandle' data-orderstep="dqy"  data-active="{{orderStep=='dqy' ? 'active' :''}}">待签约</text>
    </view>
  </view>

  <view class='plist-nav-sub' wx:if="{{orderType=='yjs' ? true :false}}">
    <view class='item-cell {{orderStep=="all" ? "active" :""}}'>
      <text bindtap='navSubHandle' data-orderstep="all"  data-active="{{orderStep=='all' ? 'active' :''}}">全部</text>
    </view>
    <view class='item-cell {{orderStep=="yqy" ? "active" :""}}'>
      <text bindtap='navSubHandle' data-orderstep="yqy"  data-active="{{orderStep=='yqy' ? 'active' :''}}">已签约</text>
    </view>
    <view class='item-cell {{orderStep=="void" ? "active" :""}}'>
      <text bindtap='navSubHandle' data-orderstep="void"  data-active="{{orderStep=='void' ? 'active' :''}}">未成单</text>
    </view>
    <view class='item-cell {{orderStep=="ysx" ? "active" :""}}'>
      <text bindtap='navSubHandle' data-orderstep="ysx"  data-active="{{orderStep=='ysx' ? 'active' :''}}">已失效</text>
    </view>
  </view>
</view>
<view class='header-top-seat {{orderType}}'></view>
<view class="coms-downrefresh-loading coms-downrefresh-current" wx-if="{{isDownRefresh}}" >
  <div class="k-line k-line-1"></div>
  <div class="k-line k-line-2"></div>
  <div class="k-line k-line-3"></div>
  <div class="k-line k-line-4"></div>
</view>
<view class='nodata-top' wx-if="{{isNoData}}">
  <text class='iconfont icon-none icon-cpic'></text>暂无数据！</view>
<view class="scroll-list-wrap">
  <view wx:for="{{dataInfo}}"  wx:key="id" wx:for-index="idx" wx:for-item="item">
    <view class='item-cell'>
    
      <view class='cname'>{{item.realname || "未填写"}}</view>
      <view class='cphone'>{{item.format_mobile || "未填写"}}</view>
      <view class='cyxdate clearfix'  wx-if="{{orderType=='wjd'? true :false}}">
        <text class="txt-1">订单有效期：</text>
        <view class="cyx-remain">{{wxTimerList[item.id].formatMinute}}分{{wxTimerList[item.id].formatSecond}}秒 </view>
        <text class="txt-2">（过期后无法再接单）</text>
      </view>

      <view class='order-info'>
        <view class='info-cell clearfix'>
          <text class='clab'>小区：</text>
          <view class='cval {{item.community || "cval-no"}}'>{{item.community || '未填写'}}</view>
        </view>
        <view class='info-cell clearfix'>
          <text class='clab'>预算：</text>
          <view class='cval {{item.price || "cval-no"}}'>{{item.price || '未填写'}}</view>
        </view>
        <view class='info-cell clearfix'>
          <text class='clab'>订单号码：</text>
          <view class='cval {{item.order_no || "cval-no"}}'>{{item.order_no || '未填写'}}</view>
        </view>
        <view class='info-cell clearfix'>
          <text class='clab'>派单时间：</text>
          <view class='cval {{item.c_time || "cval-no"}}'>{{item.c_time || '未填写'}}</view>
        </view>
      </view>

      <!--  新订单的时候显示立即接单按钮  -->
      <view class='c-jdbtn'  wx-if="{{orderType=='wjd'? true :false}}">
        <button data-id="{{item.id}}" bindtap='ljjdHandleBtn'>立即接单</button>
      </view>
      <!--  处理中要显示 步骤剩余时间  步骤按钮 -->
     
      <view class='timeremain clearfix'  wx-if="{{orderType=='clz' && item.steptype !='dqy' && item.d_time}}">
        <text class='clab'>{{item.d_time >= 0 ? "剩余时间":"已超时"}}:</text>
        <view class='c-timing'>{{wxTimerList[item.id].formatDay}}天{{wxTimerList[item.id].formatHour}}时{{wxTimerList[item.id].formatMinute}}分{{wxTimerList[item.id].formatSecond}}秒</view>
      </view>
      <navigator url='/pages/jiaju/detail/detail?id={{item.id}}' class='cnavigator' wx-if="{{item.step!='wjd'? true :false}}">{{item.format_stepname}}
        <text class='iconfont icon-jiantou txt-arrow'></text>
      </navigator>
       <view class="cmakecall" data-phone="{{item.mobile}}" bindtap='makePhoneCall' wx-if="{{item.step!='wjd' && item.step !='void' && item.step !='ysx'}}">
      <text class='iconfont icon-phone-1 icon-cpic'></text>联系客户</view>
    </view>
    <!--联系客户  处理中和已结束才会有的  -->
   
  </view>
