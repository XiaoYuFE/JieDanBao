<view class='header-top'>
  <view class="plist-nav-top">
    <view bindtap='navMainHandle' data-tabtype="xdd" class="item-cell {{tabType=='xdd' ? 'active' :''}}">新订单</view>
    <view bindtap='navMainHandle' data-tabtype="clz" class="item-cell {{tabType=='clz' ? 'active' :''}}">处理中</view>
    <view bindtap='navMainHandle' data-tabtype="yjs" class="item-cell {{tabType=='yjs' ? 'active' :''}}">已结束</view>
  </view>
  <view class='plist-nav-sub' wx:if="{{tabType=='clz' ? true :false}}">
    <view class='item-cell active'>
      <navigator hover-class="n-hover">全部</navigator>
    </view>
    <view class='item-cell'>
      <navigator hover-class="n-hover">待量房</navigator>
    </view>
    <view class='item-cell'>
      <navigator hover-class="n-hover">待设计</navigator>
    </view>
    <view class='item-cell'>
      <navigator hover-class="n-hover">待设计</navigator>
    </view>
  </view>

  <view class='plist-nav-sub' wx:if="{{tabType=='yjs' ? true :false}}">
    <view class='item-cell active'>
      <navigator hover-class="n-hover">全部</navigator>
    </view>
    <view class='item-cell'>
      <navigator hover-class="n-hover">已签约</navigator>
    </view>
    <view class='item-cell'>
      <navigator hover-class="n-hover">未成单</navigator>
    </view>
    <view class='item-cell'>
      <navigator hover-class="n-hover">已失效</navigator>
    </view>
  </view>
</view>
<view class='header-top-seat {{tabType}}'></view>


