<view class='header-top'>
  <view class="plist-nav-top">
    <view bindtap='navMainHandle' data-tabtype="xdd" class="item-cell {{tabType=='xdd' ? 'active' :''}}">新订单</view>
    <view bindtap='navMainHandle' data-tabtype="clz" class="item-cell {{tabType=='clz' ? 'active' :''}}">处理中</view>
    <view bindtap='navMainHandle' data-tabtype="yjs" class="item-cell {{tabType=='yjs' ? 'active' :''}}">已结束</view>
  </view>
  <view class='plist-nav-sub' wx:if="{{tabType=='clz' ? true :false}}">
    <view class='item-cell active'>
      <navigator hover-class="n-hover">全部</navigator>
    </view>
    <view class='item-cell'>
      <navigator hover-class="n-hover">待量房</navigator>
    </view>
    <view class='item-cell'>
      <navigator hover-class="n-hover">待设计</navigator>
    </view>
    <view class='item-cell'>
      <navigator hover-class="n-hover">待签约</navigator>
    </view>
  </view>

  <view class='plist-nav-sub' wx:if="{{tabType=='yjs' ? true :false}}">
    <view class='item-cell active'>
      <navigator hover-class="n-hover">全部</navigator>
    </view>
    <view class='item-cell'>
      <navigator hover-class="n-hover">已签约</navigator>
    </view>
    <view class='item-cell'>
      <navigator hover-class="n-hover">未成单</navigator>
    </view>
    <view class='item-cell'>
      <navigator hover-class="n-hover">已失效</navigator>
    </view>
  </view>
</view>
<view class='header-top-seat {{tabType}}'></view>

<view class='nodata-top' wx-if="{{isNoData}}">
  <text class='iconfont icon-none icon-cpic'></text>暂无数据！</view>
<view class="scroll-list-wrap">
  <view wx:for="{{dataInfo}}" wx:key="id" wx:for-index="idx" wx:for-item="item">
    <view class='item-cell'>
      <view class='cname'>{{item.name}}</view>
      <view class='cphone'>{{item.format_mobile}}</view>
      <view class='cyxdate clearfix'>
        <text class="txt-1">订单有效期：</text>
        <view class="cyx-remain">29分13秒 </view>
        <text class="txt-2">（过期后无法再接单）</text>
      </view>

      <view class='order-info'>
        <view class='info-cell clearfix'>
          <text class='clab'>小区：</text>
          <view class='cval'>{{item.community}}</view>
        </view>
        <view class='info-cell clearfix'>
          <text class='clab'>预算：</text>
          <view class='cval'>{{item.price}}</view>
        </view>
        <view class='info-cell clearfix'>
          <text class='clab'>订单号码：</text>
          <view class='cval'>{{item.order_num}}</view>
        </view>
        <view class='info-cell clearfix'>
          <text class='clab'>派单时间：</text>
          <view class='cval'>{{send_time}}</view>
        </view>
      </view>
      <view class='c-jdbtn'>
        <button data-id="{{item.id}}">立即接单</button>
      </view>
    </view>
  </view>
