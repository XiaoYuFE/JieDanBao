.avatar-box {
  padding: 40rpx 30rpx 40rpx 150rpx;
  position: relative;
  border-bottom: 20rpx solid #f7f7f7;
}

.avatar-box .pic {
  width: 100rpx;
  height: 100rpx;
  overflow: hidden;
  border-radius: 100%;
  background: #fff;
  border: 1px solid #d6d6d6;
  position: absolute;
  left: 30rpx;
  top: 50%;
  -webkit-transform: translate(0, -50%);
  transform: translate(0, -50%);
}

.avatar-box .pic image {
  width: 100%;
  height: 100%;
}

.avatar-box .hy-tag {
  height: 30rpx;
  color: #ffb759;
  font-size: 22rpx;
  padding: 0 10rpx;
  border: 1px solid #ffb759;
  border-radius: 5rpx;
  display: inline-block;
}

.goset-btn {
  position: absolute;
  right: 30rpx;
  top: 50%;
  -webkit-transform: translate(0, -50%);
  transform: translate(0, -50%);
  color: #333;
  font-size: 32rpx;
}

.goset-btn .txt-arrow {
  display: inline-block;
  vertical-align: middle;
  margin-left: 6rpx;
  font-size: 30rpx;
  margin-top: -2rpx;
}

.avatar-box .txt {
  color: #333;
  font-size: 36rpx;
  font-weight: bold;
  margin-top: 5rpx;
}

.list-type-1 {
  background: #fff;
