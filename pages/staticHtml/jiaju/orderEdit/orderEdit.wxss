.orderstep-list {
  background: #fff;
}

.orderstep-list .item-cell {
  position: relative;
  padding: 25rpx 0;
  border-bottom: 1rpx solid #e5e5e5;
}



.orderstep-list .lineh {
  content: "";
  width: 2rpx;
  top: 0;
  bottom: 0;
  left: -0.5rpx;
  background: #3ac6b8;
  position: absolute;
}

.orderstep-list .item-cell:first-child .lineh {
  top: 40rpx;
}

.orderstep-list .item-cell:last-child .lineh {
  height: 53rpx;
}

.orderstep-list .txt-statu.success {
  color: #3ac6b8;
}

.orderstep-list .name {
  color: #333;
  font-size: 30rpx;
  padding-left: 30rpx;
  position: relative;
}

.orderstep-list .time-finsh {
  padding-left: 30rpx;
  color: #999;
  font-size: 26rpx;
  margin-top: 10rpx;
}

.orderstep-list .agree-price {
  padding-left: 30rpx;
  color: #999;
  font-size: 26rpx;
  margin-top: 10rpx;
}

.orderstep-list .txt-statu {
  color: #999;
  position: absolute;
  right:30rpx;
  top: 50%;
  transform: translate(0, -50%);
  font-size: 28rpx;
}

.orderstep-list .txt-statu.success-top {
  top: 55rpx;
  color: #3ac6b8;
}
