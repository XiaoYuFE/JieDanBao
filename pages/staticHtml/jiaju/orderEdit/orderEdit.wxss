  position: fixed;
  color: #fff;
  left: 0;
  bottom: 0;
  background: #fff;
  right: 0;
  padding: 20rpx 30rpx;
}

.fixed-bottom-btn .btn-update {
  height: 88rpx;
  line-height: 88rpx;
  text-align: center;
  background: #3ac6b8;
  border-radius: 10rpx;
}


.cell-radio-box{
   position:absolute;
   right:30rpx;
   top:25rpx;
   font-size: 28rpx;  
}




radio-group .radio{
  position:relative;
  padding-left: 55rpx; 
  margin-left:25rpx; 
}
radio .wx-radio-input{
   border-radius: 50%;
   width: 42rpx;
   height: 42rpx;
   box-sizing: border-box;
   border:2rpx solid #cacaca;
   position: absolute;
   left:0;
   top: 50%;
   -webkit-transform: translate(0,-50%);
    transform: translate(0,-50%);
}
radio.disabled .wx-radio-input{
  background:#e1e1e1;
  border-color:#cacaca; 
}
radio .wx-radio-input.wx-radio-input-checked::before{
   content: "\e600";
   font-family:"iconfont";
   font-size:48rpx;
   color:#3ac6b8;  
   transform:translate(-50%, -50%);
   -webkit-transform:translate(-50%, -50%) ;
}
radio.disabled .wx-radio-input.wx-radio-input-checked::before{
    color:#e1e1e1; 
}

/**量房时间**/

.cell-lf-time{
  padding:20rpx  0 0;

  color:#333333; 
  line-height:40rpx;
  border-top:1px solid #e5e5e5; 
  margin:25rpx 30rpx 0;
  
  
}
.cell-lf-time .lab{
    float:left; 
    font-size:26rpx;
}
.cell-lf-time .time-picker{
    float:right;
    font-size:28rpx; 
    position: relative; 
}
.cell-lf-time .time-picker picker{
  padding-right: 40rpx;
  
}

.cell-lf-time .time-picker .txt-arrow{
    position: absolute;
    right:-20rpx;
    top: 50%;
    -webkit-transform: translate(-50%,-50%);
    transform: translate(-50%,-50%); 
}
